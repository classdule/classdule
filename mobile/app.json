{
  "expo": {
    "name": "Classdule",
    "slug": "classdule",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/classdule-icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/classdule-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#161616"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/classdule-adaptive-icon.png",
        "backgroundColor": "#161616"
      }
    },
    "web": {
      "favicon": "./assets/classdule-favicon.png"
    }
  }
}
